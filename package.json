    "@types/react": "^0.14.54",
    "@types/react-dom": "^0.14.19",
    "@types/react-router": "^3.0.1",
    "@types/react-router-redux": "^4.0.40",
    "@types/webpack": "^2.2.7",
    "@types/winston": "^2.2.0",
    "ava": "^0.18.2",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-array-includes": "^2.0.3",
