{
  "name": "wokugame",
  "version": "1.0.0",
  "description": "woku games",
  "scripts": {
    "tsc": "rm -rf built && tsc",
    "tsc-watch": "tsc -w",
    "dll": "webpack --config built/scripts/webpack/webpack.dll.config.js",
    "dll-production": "webpack --config built/scripts/webpack/webpack.dll.production.config.js",
    "start-before": "npm-run-all --sequential tsc dll",
    "server": "node built/src/server/index.js",
    "server-production": "node built/src/server/index.js --production",
    "webpack": "node built/scripts/webpack/webpack-server.js",
    "webpack-production": "webpack --config built/scripts/webpack/webpack.production.config.js",
    "gulp": "gulp --gulpfile built/scripts/gulp/gulpfile.js --cwd ./",
    "gulp-production": "gulp production --gulpfile built/scripts/gulp/gulpfile.js --cwd ./",
    "production": "npm-run-all --sequential -l tsc dll-production gulp-production webpack-production",
    "start": "npm-run-all --parallel -l server gulp webpack tsc-watch",
    "pm2-start": "pm2 start /app/built/src/server/index.js -- --production",
    "pm2-restart": "pm2 restart all",
    "pm2-stop": "pm2 stop all",
    "pm2-list": "pm2 list",
    "deploy": "pm2-docker /app/built/deploy/index.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "github-webhook-handler": "^0.6.0",
    "graphql": "^0.8.2",
    "graphql-server-koa": "^0.4.3",
    "immutable": "^3.8.1",
    "invariant": "^2.2.2",
    "keymaster": "^1.6.2",
    "koa": "^1.2.1",
    "koa-proxy": "^0.6.0",
    "koa-router": "^5.4.0",
    "koa-static-cache": "^3.1.7",
    "lodash": "^4.17.2",
    "moment": "^2.15.0",
    "pixi.js": "^4.3.0",
    "pm2": "^2.2.3",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.6.0",
    "redux-observable": "^0.12.2",
    "rxjs": "^5.0.0-rc.5",
    "seamless-immutable": "^7.0.1"
  },
  "devDependencies": {
    "@types/gulp": "^3.8.32",
    "@types/invariant": "^2.2.28",
    "@types/koa": "^0.0.0",
    "@types/koa-router": "^7.0.21",
    "@types/node": "^0.0.2",
    "@types/pixi.js": "^4.1.0",
    "@types/react": "^0.14.54",
    "@types/react-dom": "^0.14.19",
    "@types/react-router": "^2.0.41",
    "@types/react-router-redux": "^4.0.35",
    "@types/seamless-immutable": "^6.1.2",
    "@types/webpack": "^2.1.0",
    "autoprefixer": "^6.4.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-assign": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "format-json": "^1.0.3",
    "gulp": "^3.9.1",
    "gulp-cached": "^1.1.1",
    "gulp-nodemon": "^2.2.1",
    "happypack": "^3.0.1",
    "json-loader": "^0.5.4",
    "npm-run-all": "^4.0.0",
    "react-addons-perf": "^15.4.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "text-loader": "0.0.1",
    "typescript": "^2.1.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.14.1"
  }
}
